# Environment variables template
# Copy this to .env and fill in your values

# Server Configuration
PORT=5000
NODE_ENV=production

# Database
MONGODB_URI=mongodb+srv://<username>:<password>@cluster.mongodb.net/codesphere?retryWrites=true&w=majority

# JWT Authentication
JWT_SECRET=your_super_secure_jwt_secret_key_at_least_32_characters_long

# CORS - Frontend URL
FRONTEND_URL=https://your-frontend-domain.vercel.app

# Code Execution (Optional - for Judge0 integration)
JUDGE0_API_KEY=your_judge0_api_key_if_using
JUDGE0_API_URL=https://judge0-ce.p.rapidapi.com

# Socket.IO Configuration (Optional)
SOCKET_CORS_ORIGIN=https://your-frontend-domain.vercel.app
